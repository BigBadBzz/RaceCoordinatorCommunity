    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="RaceResultsAll" Height="1000" Width="1280" WindowState="Maximized" Background="Transparent" WindowStartupLocation="CenterScreen">

    <Window.Resources>
        <Brush x:Key="LabelColor">#FFFFFFFF</Brush>
        <Brush x:Key="ValueColor">#FFFFFFFF</Brush>

        <Brush x:Key="RSLabelColor">#FFFFFFFF</Brush>
        <Brush x:Key="RSValueColor">#FFFFFFFF</Brush>
        <Brush x:Key="RSBorderBrush">#FFFFFFFF</Brush>


        <Brush x:Key="RSTopGridBackground">#00000000</Brush>
        <Brush x:Key="RSTableLabelBackground">#00000000</Brush>
        <Brush x:Key="RSTableCellBackground">#00000000</Brush>

    </Window.Resources>

    <Viewbox Stretch="Fill">
        <Grid>
<!--            <Grid.Background>   (This has been commented out to allow photographs to be take - DGMiller June 2016
<LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1" SpreadMethod="Pad" ColorInterpolationMode="SRgbLinearInterpolation" >
    <GradientStop Color="#FF181616" Offset="0" />
    <GradientStop Color="#FF000000" Offset="1" />
    <GradientStop Color="#FF2DADD6" Offset="0.5" />
</LinearGradientBrush>
            </Grid.Background> -->

    <Grid.RowDefinitions>
        <RowDefinition Height="75"/>   <!-- Top StackPanel - Track, Race Name and Best time -->
        <RowDefinition Height="45"/>   <!--  -->
        <RowDefinition Height="600"/>
    </Grid.RowDefinitions>

    <Grid.ColumnDefinitions>
        <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Grid Grid.Row="0">
    <Grid.ColumnDefinitions>
        <ColumnDefinition Width="400" />
        <ColumnDefinition Width="700"/>

</Grid.ColumnDefinitions>


<Viewbox Stretch="Fill" Grid.Column="0">
    <StackPanel Grid.Column="0" VerticalAlignment="Top" Background="Transparent" >
<!--        <Viewbox Margin="5,0,25,0" Stretch="None">
            <Image Name="TrackImage_1" VerticalAlignment="Center" Margin="15,0,0,0" Height="100" StretchDirection="Both" MaxWidth="300" />
        </Viewbox> -->
        <Viewbox MaxWidth="400"  MaxHeight="60" MinHeight="60">
            <Label Margin="5,0,25,0" Name="TrackName_1" VerticalAlignment="Top" Foreground="Lime" FontSize="30" HorizontalContentAlignment="Center" HorizontalAlignment="Stretch" VerticalContentAlignment="Top" Padding="2" FontWeight="Bold" FontStyle="Italic" />
        </Viewbox>
        <Viewbox MaxWidth="400" MaxHeight="45">
            <Label Background="Transparent" BorderBrush="Transparent" BorderThickness="2" FontSize="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" Name="RaceName_1" Foreground="DodgerBlue" FontWeight="Bold"></Label>
        </Viewbox>
    </StackPanel>
</Viewbox>

<StackPanel Orientation="Vertical" Background="Transparent" Grid.Column="1" VerticalAlignment="Center">
    <TextBlock FontSize="20" Foreground="Tomato" Text="Race Record Lap" FontWeight="Bold" Margin="75, 0, 5, 0" />
    <DockPanel LastChildFill="False" Margin="25, 0, 5, 0">
            <TextBlock FontSize="14" Name="RecordName_1" DockPanel.Dock="Left" Margin="75,0,10,0" Foreground="Tomato" />
            <TextBlock FontSize="14" Foreground="Tomato" Text="("/>
            <TextBlock FontSize="14" Name="RecordNickname_1" Foreground="Tomato" />
            <TextBlock FontSize="14" Foreground="Tomato" Text="): "/>
            <TextBlock FontSize="14" Name="RecordTime_1" Foreground="Tomato"  DataContext="'decimals':3"/>
        </DockPanel>
</StackPanel>

<StackPanel Orientation="Vertical" Background="Transparent" Grid.Column="1" VerticalAlignment="Center">
    <TextBlock FontSize="20" Foreground="Lime" Margin="300, 0, 5, 0" Text="Current Race Best Lap" FontWeight="Bold"/>
    <DockPanel  LastChildFill="False" Margin="0, 0, 5, 0">
            <TextBlock FontSize="14" Name="BestName_1" DockPanel.Dock="Left" Margin="300,0,5,0" Foreground="Lime"/>
            <TextBlock FontSize="14" Foreground="Lime" Text="("/>
            <TextBlock FontSize="14" Name="BestNickname_1" Foreground="Lime" />
            <TextBlock FontSize="14" Foreground="Lime" Text="): "/>
            <TextBlock FontSize="14" Name="BestTime_1" Foreground="Lime"  DataContext="'decimals':3"/>
        </DockPanel>
</StackPanel>
            </Grid>
