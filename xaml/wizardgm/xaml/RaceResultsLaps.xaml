<Window
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="RaceResultsLaps" Height="1000" Width="1280" WindowState="Maximized" Background="Transparent" WindowStartupLocation="CenterScreen">

    <Window.Resources>
        <Brush x:Key="LabelColor">#FFFFFFFF</Brush>
        <Brush x:Key="ValueColor">#FFFFFFFF</Brush>

        <Brush x:Key="RSLabelColor">#FFFFFFFF</Brush>
        <Brush x:Key="RSValueColor">#FFFFFFFF</Brush>
        <Brush x:Key="RSBorderBrush">#FFFFFFFF</Brush>


        <Brush x:Key="RSTopGridBackground">#00000000</Brush>
        <Brush x:Key="RSTableLabelBackground">#00000000</Brush>
        <Brush x:Key="RSTableCellBackground">#00000000</Brush>

    </Window.Resources>

    <Viewbox Stretch="Fill">
<Grid>
    <Grid.RowDefinitions>
        <RowDefinition Height="75"/>
        <RowDefinition Height="46"/>
        <RowDefinition Height="560"/>
    </Grid.RowDefinitions>

    <Grid.ColumnDefinitions>
        <ColumnDefinition/>
    </Grid.ColumnDefinitions>

<Grid Grid.Row="0">
    <Grid.ColumnDefinitions>
        <ColumnDefinition Width="400" />
        <ColumnDefinition Width="700"/>
    </Grid.ColumnDefinitions>


<Viewbox Stretch="Fill" Grid.Column="0">
    <StackPanel Grid.Column="0" VerticalAlignment="Top" Background="Transparent" >
        <Viewbox Margin="5,0,25,0" Stretch="None">
<!--            <Image Name="TrackImage_1" VerticalAlignment="Center" Margin="15,0,0,0" Height="100" StretchDirection="Both" MaxWidth="300" /> -->
        </Viewbox>
        <Viewbox MaxWidth="400"  MaxHeight="60" MinHeight="60">
            <Label Margin="5,0,25,0" Name="TrackName_1" VerticalAlignment="Top" Foreground="Lime" FontSize="30" HorizontalContentAlignment="Center" HorizontalAlignment="Stretch" VerticalContentAlignment="Top" Padding="2" FontWeight="Bold" FontStyle="Italic" />
        </Viewbox>
        <Viewbox MaxWidth="400" MaxHeight="45">
            <Label Background="Transparent" BorderBrush="Transparent" BorderThickness="2" FontSize="25" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" Name="RaceName_1" Foreground="Dodgerblue" FontWeight="Bold"></Label>
        </Viewbox>
    </StackPanel>
</Viewbox>

<!-- Class and race records - DGM March 2023 -->
<StackPanel Orientation="Vertical" Background="Transparent" Grid.Column="1" VerticalAlignment="Center">
    <TextBlock FontSize="20" Foreground="Tomato" Text="ClassRecordLap" FontWeight="Bold" Margin="75, 0, 5, 0" />
    <DockPanel LastChildFill="False" Margin="25, 0, 5, 0">
            <TextBlock FontSize="14" Name="RecordName_1" DockPanel.Dock="Left" Margin="75,0,10,0" Foreground="Tomato" />
            <TextBlock FontSize="14" Foreground="Tomato" Text="("/>
            <TextBlock FontSize="14" Name="RecordNickname_1" Foreground="Tomato" />
            <TextBlock FontSize="14" Foreground="Tomato" Text="): "/>
            <TextBlock FontSize="14" Name="RecordTime_1" Foreground="Tomato"  DataContext="'decimals':3"/>
        </DockPanel>
</StackPanel>


<StackPanel Orientation="Vertical" Background="{StaticResource RSTopGridBackground}" Grid.Column="1" VerticalAlignment="Center">
    <TextBlock FontSize="20" Foreground="Lime" Margin="300, 0, 5, 0" Text="Current Race Best Lap" FontWeight="Bold"/>
        <DockPanel  LastChildFill="False" Margin="10, 0, 5, 0">
            <TextBlock FontSize="14" Name="BestName_1" DockPanel.Dock="Left" Margin="300,0,5,0" Foreground="Lime"/>
            <TextBlock FontSize="14" Foreground="Lime" Text="("/>
            <TextBlock FontSize="14" Name="BestNickname_1" Foreground="Lime" />
            <TextBlock FontSize="14" Foreground="Lime" Text="): "/>
            <TextBlock FontSize="14" Name="BestTime_1" Foreground="Lime"  DataContext="'decimals':3"/>
        </DockPanel>
</StackPanel>

</Grid>
<!--Labels for Column Headers-->
<StackPanel Grid.Row="1" Width="1024" Height="40" Margin="15,0,10,0" Orientation="Horizontal" HorizontalAlignment="Left">
<!--Pos-->          <Label Width="75"   Background="Transparent" BorderBrush="Transparent" BorderThickness="1" FontSize="25" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Content="Pos" Foreground="White" FontStyle="Normal" FontWeight="Bold" />
<!--Driver-->       <Label Width="275"  Background="Transparent" BorderBrush="Transparent" BorderThickness="1" FontSize="25" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Content="Driver" Foreground="White" FontStyle="Normal" FontWeight="Bold" />
<!--Laps-->         <Label Width="200"  Background="Transparent" BorderThickness="1" FontSize="25" HorizontalContentAlignment="Center"           VerticalContentAlignment="Center" Content="Laps" Foreground="White" FontStyle="Normal" FontWeight="Bold" />
<!--Average-->      <Label Width="110"  Background="Transparent" BorderThickness="1" FontSize="24" HorizontalContentAlignment="Center"           VerticalContentAlignment="Center" Foreground="White" Content="Avg Lap" FontStyle="Normal" FontWeight="Bold" />
<!--Best-->         <Label Width="130"  Background="Transparent" BorderThickness="1" FontSize="24" HorizontalContentAlignment="Center"           VerticalContentAlignment="Center" Foreground="White" Content="Best Lap" FontWeight="Bold" FontStyle="Normal" />
<!--Gap to Prev-->  <Label Width="225"  Background="Transparent" BorderBrush="Transparent" BorderThickness="1" FontSize="24" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="White" Content="Gap to Leader" FontStyle="Normal" FontWeight="Bold" />
</StackPanel>
<!--End of labels for Column Headers-->

