<!--The first tag we are using will define the application window. You can read about some of the "Window" properties
    at http://msdn.microsoft.com/en-us/library/system.windows.window.aspx -->
<Window 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:sys="clr-namespace:System;assembly=mscorlib"
    Title="Stewart Raceway Qualifying" Background="Transparent" Height="768" Width="1024" WindowState="Maximized" FontFamily="Tahoma" WindowStyle="ThreeDBorderWindow"
WindowStartupLocation="CenterScreen">

	<Window.Resources>
		<Brush x:Key="StatusGridBackground">Linen</Brush>
		<Brush x:Key="StatusGridValueColor">White</Brush>
		<!--Brush x:Key="StatusGridBorderThickness">1</Brush-->
		<Brush x:Key="StatusGridBorderBrush">Black</Brush>
	</Window.Resources>

	<Viewbox Stretch="Fill">
		<!--Main Grid -->
		<Grid ShowGridLines="False">
			<Grid.Background>
				<LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1" SpreadMethod="Pad" ColorInterpolationMode="SRgbLinearInterpolation" >
					<GradientStop Color="black" Offset="0" />
					<GradientStop Color="black" Offset="1" />
					<GradientStop Color="black" Offset="0.5" />
				</LinearGradientBrush>
			</Grid.Background>

			<Grid  Background="LightGray">
				<!--Menu Bar at top of main window. Contains race managing controls and contols for other windows such as the leaderboard.-->
				<Menu Height="15" FontSize="8" Name="menu1" VerticalAlignment="Top" HorizontalAlignment="Left">
					<MenuItem Header="File">
						<MenuItem Name="Save_1" Header="Save" DataContext="'key':'S','modifier':'Alt'" ToolTip="ALT-S to save the race" />
						<MenuItem Name="SaveAs_1" Header="Save As..." DataContext="'key':'A','modifier':'Alt'" ToolTip="ALT-a to save the race with the spefied file name" />
						<Separator/>
						<MenuItem Name="Export_1" Header="Export Race"  DataContext="'key':'X','modifier':'Alt'" ToolTip="Alt-X to export current race progress" />
					</MenuItem>
					<MenuItem Header="Race Director">
						<MenuItem Name="Start_1" Header="Start/Resume Heat" DataContext="'key':'S','modifier':'Control'" ToolTip="CTRL-S to start or restart the heat" />
						<MenuItem Name="Pause_1" Header="Pause Heat" DataContext="'key':'P','modifier':'Control'" ToolTip="CTRL-P to pause the heat" />
						<Separator/>
						<MenuItem Name="AddLaps_1" Header="Add Laps/Sections"  DataContext="'key':'A','modifier':'Control'" ToolTip="CTRL-A to add laps/sections" />
						<MenuItem Name="NextHeat_1" Header="Next Heat" DataContext="'key':'N','modifier':'Control'" ToolTip="CTRL-N to advance to the next heat" />
						<Separator/>
						<MenuItem Name="RestartHeat_1" Header="Restart Heat" DataContext="'key':'R','modifier':'Control'" ToolTip="CTRL-R to restart the current heat" />
						<MenuItem Name="DeferHeat_1" Header="Defer Heat"  DataContext="'key':'D','modifier':'Control'" ToolTip="CTRL-D to defer this heat until the end of the race" />
						<MenuItem Name="SkipHeat_1" Header="Skip Heat"  DataContext="'key':'F5','modifier':'Alt'" ToolTip="ALT-F5 to skip the remainder of this heat" />
						<MenuItem Name="SkipRace_1" Header="Skip Race"  DataContext="'key':'F4','modifier':'Alt'" ToolTip="CTRL-F4 to end this race" />
						<Separator/>
						<MenuItem Name="ModifyHeats_1" Header="Modify Heats"  DataContext="'key':'H','modifier':'Control'" ToolTip="CTRL-H to modify heat rotations" />
						<MenuItem Name="LapEditor_1" Header="Edit Laps" DataContext="'key':'E','modifier':'Control'" ToolTip="CTRL-E to modify lap records" />
					</MenuItem>
					<MenuItem Header="Track Power">
						<MenuItem CommandParameter="0" Name="PowerOn_1" Header="Master Power On" DataContext="'key':'0','modifier':'Control'" ToolTip="CTRL-0 to force master power on" />
						<MenuItem CommandParameter="0" Name="PowerOff_1" Header="Master Power Off" DataContext="'key':'0','modifier':'Alt'" ToolTip="ALT-0 to force master power off"/>
						<MenuItem CommandParameter="1" Name="PowerOn_2" Header="Lane 1 Power On" DataContext="'key':'1','modifier':'Control'" ToolTip="CTRL-1 to force lane 1 power on" />
						<MenuItem CommandParameter="1" Name="PowerOff_2" Header="Lane 1 Power Off" DataContext="'key':'1','modifier':'Alt'" ToolTip="ALT-1 to force lane 1 power off"/>
						<MenuItem CommandParameter="2" Name="PowerOn_3" Header="Lane 2 Power On" DataContext="'key':'2','modifier':'Control'" ToolTip="CTRL-2 to force lane 2 power on" />
						<MenuItem CommandParameter="2" Name="PowerOff_3" Header="Lane 2 Power Off" DataContext="'key':'2','modifier':'Alt'" ToolTip="ALT-2 to force lane 2 power off"/>
						<MenuItem CommandParameter="3" Name="PowerOn_4" Header="Lane 3 Power On" DataContext="'key':'3','modifier':'Control'" ToolTip="CTRL-3 to force lane 3 power on" />
						<MenuItem CommandParameter="3" Name="PowerOff_4" Header="Lane 3 Power Off" DataContext="'key':'3','modifier':'Alt'" ToolTip="ALT-3 to force lane 3 power off"/>
						<MenuItem CommandParameter="4" Name="PowerOn_5" Header="Lane 4 Power On" DataContext="'key':'4','modifier':'Control'" ToolTip="CTRL-4 to force lane 4 power on" />
						<MenuItem CommandParameter="4" Name="PowerOff_5" Header="Lane 4 Power Off" DataContext="'key':'4','modifier':'Alt'" ToolTip="ALT-4 to force lane 4 power off"/>
					</MenuItem>
					<Separator Width="5" />
					<MenuItem Header="Windows">
						<MenuItem Name="Window_1" Header="Leader Board" CommandParameter="data/xaml/LeaderBoard.xaml"  DataContext="'key':'F1'" ToolTip="F1 to display the Leader Board" />
						<MenuItem Name="Window_2" Header="Top 5" CommandParameter="data/xaml/Top5.xaml"  DataContext="'key':'F1','modifier':'Control'" ToolTip="CTRL F1 to display the Top 5 Leader Board" />
						<MenuItem Name="Window_3" Header="Group Leader Board" CommandParameter="data/xaml/GroupLeaderBoard.xaml"  DataContext="'key':'F3'" ToolTip="F3 to display the Group Leader Board" />
						<MenuItem Name="Window_4" Header="Heat Results" CommandParameter="data/xaml/HeatResults.xaml" DataContext="'key':'F4'" ToolTip="F4 to display the Heat Results window" />
						<MenuItem Name="Window_5" Header="Race Results" CommandParameter="data/xaml/RaceResultsV1.xaml" DataContext="'key':'F5'" ToolTip="F5 to display the Race Results window" />
						<Separator/>
						<MenuItem Name="Window_6" Header="Season Race Leader Board" CommandParameter="data/xaml/SeasonRaceLeaderBoard.xaml"  DataContext="'key':'F1' ,'modifier':'Alt'" ToolTip="ALT F1 to display the Season Race Leader Board" />
						<MenuItem Name="Window_7" Header="Season Leader Board" CommandParameter="data/xaml/SeasonLeaderBoard.xaml"  ToolTip="Display the Season Leader Board" />
						<Separator/>
						<MenuItem Name="Window_8" Header="On Deck" CommandParameter="data/xaml/OnDeck.xaml" DataContext="'key':'F6'" ToolTip="F6 to display the On Deck window" />
						<MenuItem Name="ViewHeats_1" Header="Heat List" DataContext="'key':'F7'" ToolTip="F7 to display heat rotations" />
						<MenuItem Name="Window_9" Header="Next Heat" CommandParameter="data/xaml/NextHeat_4L.xaml" DataContext="'key':'F8'" ToolTip="F8 to display all drivers in the next heat" />
					</MenuItem>
				</Menu>
			</Grid>

			<!-- MAIN GRID OF SCREEN -->
			<Grid Name="containergrid1" ShowGridLines="False" Margin="0,13,0,0" Background="LightGray">
				<Grid.RowDefinitions>
					<RowDefinition Height="120" />
					<RowDefinition Height="Auto" />
					<RowDefinition Height="*"/>
					<RowDefinition Height="25" />
				</Grid.RowDefinitions>
				<Grid.ColumnDefinitions>
					<ColumnDefinition />
					<ColumnDefinition MinWidth="170"/>
					<ColumnDefinition MinWidth="170"/>
					<ColumnDefinition MinWidth="170"/>
				</Grid.ColumnDefinitions>
				<!-- SETS SHADING ON BOTTOM ROW OF GRID -->
				<Border Grid.Row="3" Grid.ColumnSpan="4">
					<Border.Background>
						<LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
							<GradientStop Color="Black" Offset="0" />
							<GradientStop Color="White" Offset="1" />
						</LinearGradientBrush>
					</Border.Background>
				</Border>

				<!-- SETS UP TOP ROW OF SCREEN -->

				<!-- CURRENT DRIVER STATUS BLOCK -->
				<StackPanel Grid.Row="0" Grid.Column="1" Margin="3" Background="Navy" Width="210" Height="200" MinWidth="180" HorizontalAlignment="Center">
					<Grid ShowGridLines="False">
						<Grid.RowDefinitions>
							<RowDefinition Height="*"/>
							<RowDefinition Height="40"/>
							<RowDefinition Height="40"/>
						</Grid.RowDefinitions>
						<Grid.ColumnDefinitions>
							<ColumnDefinition Width="*"/>
							<ColumnDefinition Width="*"/>
						</Grid.ColumnDefinitions>
						<TextBlock Grid.Row="0" Grid.ColumnSpan="2" FontSize="18" Text="Current Driver" Margin="4" Foreground="White" HorizontalAlignment="Center"/>
						<!-- BEST LAP TIME -->
						<Viewbox Grid.Row="1" Grid.Column="0" Stretch="Fill">
						<TextBlock FontSize="26" Name="BestLapTime_Position1_1" Margin="4,0,1,0" Padding="0" VerticalAlignment="Center" Foreground="White" Background="Navy" HorizontalAlignment="Center" DataContext="'decimals':3"/>
						</Viewbox>
						<!-- BEST LAP MPH -->
						<StackPanel Grid.Row="1" Grid.Column="1" Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Center">
							<TextBlock FontSize="22" Name="MPH_Position1_1" Margin="2" Padding="3,0,3,0" Background="Navy" Foreground="White" DataContext="'decimals':1"/>
							<TextBlock FontSize="10" Text="MPH" Margin="0" Foreground="White" VerticalAlignment="Center" HorizontalAlignment="Center"/>
						</StackPanel>
						<!-- DRIVER NAME -->
						<Viewbox Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="2" Stretch="Fill" Width="Auto" Height="Auto" Margin="2,8,2,2">
						<TextBlock Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="2" FontSize="20" FontWeight="Bold" Name="Nickname_Position1_1" Margin="2,2,2,2" Padding="1" Background="Blue" Foreground="White" HorizontalAlignment="Center" 
						VerticalAlignment="Center"/>
						</Viewbox>
					</Grid>
				</StackPanel>

				<!-- RACE NAME, HEAT STATUS, FLAG STATUS, ON DECK, RACE TIME -->
				<StackPanel Grid.Row="0" Grid.Column="2" Margin="5" HorizontalAlignment="Center" Height="Auto" Background="Navy" >
					<Grid>
						<Grid.RowDefinitions>
							<RowDefinition Height="*"/>
							<RowDefinition Height="*"/>
							<!--RowDefinition Height="*"/-->
						</Grid.RowDefinitions>
						<Grid.ColumnDefinitions>
							<ColumnDefinition Width="75"/>
							<ColumnDefinition Width="75"/>
							<ColumnDefinition Width="75"/>
						</Grid.ColumnDefinitions>
						<!-- RACE NAME -->
						<TextBlock Grid.Row="0" Grid.ColumnSpan="3" FontSize="20" Name="RaceName_1" Margin="4" Background="Transparent" FontWeight="Bold" Foreground="White" HorizontalAlignment="Center"/>
						<DockPanel Grid.Row="1" Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Center">
							<TextBlock FontSize="16" FontWeight="Bold" DockPanel.Dock="Top" Foreground="#00FF05" Text="Driver" HorizontalAlignment="Center" VerticalAlignment="Center"/>
							<TextBlock FontSize="16" FontWeight="Bold"  Name="HeatNumber_1" DockPanel.Dock="Left" Foreground="#00FF05" HorizontalAlignment="Center" VerticalAlignment="Center" />
							<TextBlock FontSize="16" FontWeight="Bold"  Name="NumHeats_1" DockPanel.Dock="Right" Foreground="#00FF05" HorizontalAlignment="Center" VerticalAlignment="Center"/>
							<TextBlock FontSize="10"  Foreground="#00FF05" Text=" of " HorizontalAlignment="Center" VerticalAlignment="Center" />
						</DockPanel>
						<!-- RACE STATUS FLAG -->
						<Image Grid.Row="1" Grid.Column="1" Name="RaceStateImage_1" Margin="0" Stretch="Fill" VerticalAlignment="Center" HorizontalAlignment="Center" MaxWidth="75" MaxHeight="58"/>
						<!-- RACE TIME -->
						<Label Grid.Row="1" Grid.Column="2" Margin="0" Background="Transparent" BorderBrush="Transparent" BorderThickness="1" FontSize="25" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Center" Name="RaceTime_1" Foreground="#00FF05" FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Center">
						</Label>
					</Grid>
				</StackPanel>
				<!-- RACE BEST LAP/POLE POSITION -->
				<StackPanel Grid.Row="0" Grid.Column="3" Margin="5" MinWidth="170" MaxWidth="175" Background="Green" HorizontalAlignment="Center">
					<TextBlock FontSize="22" Text="Pole Position" Margin="4,4,4,0" Foreground="White" HorizontalAlignment="Center"/>
					<TextBlock FontSize="28" Name="AverageTime_RaceLeader1_3" Margin="5,1,5,1" Foreground="White" HorizontalAlignment="Center" DataContext="'decimals':3"/>
					<Viewbox  Stretch="Fill" Width="Auto" Height="40" Margin="2,2,2,2">
						<TextBlock FontSize="20" FontWeight="Bold" Name="Nickname_RaceLeader1_4" Margin="2,2,2,2" Padding="1" Foreground="White" HorizontalAlignment="Center" 
						VerticalAlignment="Center"/>
						</Viewbox>
				</StackPanel>
				<!-- END OF TOP ROW GRID -->


				<!-- BEGIN MIDDLE GRID ROW FOR RACE STATUS AND DRIVER RESULTS -->
				<!-- StackPanel Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="2" -->

				<!-- RACE STATUS BAR ABOVE DRIVER RESULTS GRID -->
				<DockPanel Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="2" Height="25" Width="430" Margin="5,5,2,5" Background="Navy" LastChildFill="False" VerticalAlignment="Top" >
					<Label DockPanel.Dock="Left" Content="Race Laps/Time: " FontSize="10" FontWeight="Bold" VerticalAlignment="Center"
								Foreground="Yellow"/>
					<TextBlock DockPanel.Dock="Left" Name="EndHeatValue_1" FontSize="15" FontWeight="Bold" VerticalAlignment="Center" Foreground="#00FF05"/>
					<!-- LAPS/TIME LEFT -->
					<TextBlock DockPanel.Dock="Right" Name="RaceLaps_1" VerticalAlignment="Center" MaxWidth="55" Margin="0,0,5,0" FontSize="15" Foreground="#00FF05" FontWeight="Bold" DataContext="'decimals':2"/>
					<Label DockPanel.Dock="Right" Content="Laps/Time Left: " FontSize="10" FontWeight="Bold" Foreground="Yellow" VerticalAlignment="Center"/>
					<!-- ON DECK  -->
					<StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="6,0,0,0">
						<TextBlock FontSize="10" Foreground="#ffef0d" Text="ON DECK:" Margin="10,0,2,0" FontWeight="Bold" VerticalAlignment="Center" HorizontalAlignment="Center" />
						<Viewbox Margin="2" HorizontalAlignment="Center" VerticalAlignment="Center" Stretch="Fill" MaxWidth="138"  MinWidth="138" MaxHeight="18" MinHeight="14">
							<Border BorderThickness=".5" BorderBrush="Gray" Padding="2,1,0,1" HorizontalAlignment="Center">
								<TextBlock Name="OnDeckNickname1_1" Margin="0,0,2,0" Padding="6,1,6,1" FontSize="14" Foreground="#00FF05" Background="Purple" HorizontalAlignment="Center" VerticalAlignment="Center"/>
							</Border>
						</Viewbox>
					</StackPanel>
					<!-- END TAG FOR RACE STATUS BAR -->
				</DockPanel>

				<!-- DRIVER RESULTS HEADERS GRID -->
				<Grid Grid.Column="1" Grid.Row="2" Grid.ColumnSpan="2" Margin="5,0,5,0" VerticalAlignment="Top">
					<Grid.RowDefinitions>
						<RowDefinition Height="20" />
					</Grid.RowDefinitions>
					<Grid.ColumnDefinitions>
						<ColumnDefinition Width="20" />
						<ColumnDefinition Width="145" />
						<ColumnDefinition Width="30" />
						<ColumnDefinition Width="70" />
						<ColumnDefinition Width="70" />
						<ColumnDefinition Width="70" />
						<ColumnDefinition Width="70" />
					</Grid.ColumnDefinitions>

					<!--Labels for Column Headers (DUPLICATE) -->

					<Label Padding="0,-3,0,0" Grid.Column="0" Grid.Row="0" Background="DarkGray" BorderBrush="DarkGray" BorderThickness="1" FontSize="10" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Content="Pos" Foreground="White" FontStyle="Normal" FontWeight="Bold" />
					<Label Padding="0,-3,0,0" Grid.Column="1" Grid.Row="0" Background="DarkGray" BorderBrush="DarkGray" BorderThickness="1" FontSize="10" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Content="Driver" Foreground="White" FontStyle="Normal" FontWeight="Bold" />
					<Label Padding="0,-3,0,0" Grid.Column="2" Grid.Row="0" Background="DarkGray" BorderBrush="DarkGray" BorderThickness="1" FontSize="8" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Content="Lap" Foreground="White" FontStyle="Normal" FontWeight="Bold" />
					<Label Padding="0,-3,0,0" Grid.Column="3" Grid.Row="0" Background="DarkGray" BorderBrush="DarkGray" BorderThickness="1" FontSize="10" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="White" Content="Avg Time" FontStyle="Normal" FontWeight="Bold" />
					<Label Padding="0,-3,0,0" Grid.Column="4" Grid.Row="0" Background="DarkGray" BorderBrush="DarkGray" BorderThickness="1" FontSize="9" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="White" Content="Best Time" FontWeight="Bold" FontStyle="Normal" />
					<Label Padding="0,-3,0,0" Grid.Column="5" Grid.Row="0" Background="DarkGray" BorderBrush="DarkGray" BorderThickness="1" FontSize="10" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="White" Content="Gap 1st" FontWeight="Bold" FontStyle="Normal" />
					<Label Padding="0,-3,0,0" Grid.Column="6" Grid.Row="0" Background="DarkGray" BorderBrush="DarkGray" BorderThickness="1" FontSize="10" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="White" Content="Gap" FontStyle="Normal" FontWeight="Bold" />
					<!--End of labels for Column Headers-->
				</Grid>

				<!-- DRIVER RESULTS DATA GRID -->
				<Border Grid.Column="1" Grid.Row="2" Grid.ColumnSpan="2" Margin="5,0,5,5" BorderBrush="DarkGray" BorderThickness="1.5">
					<ScrollViewer Height="200" VerticalScrollBarVisibility="Hidden" Margin="0,0,0,0" Padding="0" VerticalAlignment="Center">
						<ScrollViewer.Resources>
							<sys:Double x:Key="{x:Static SystemParameters.VerticalScrollBarWidthKey}">8</sys:Double>
						</ScrollViewer.Resources>

						<Grid Name="grid2" Margin="0,16,0,0" HorizontalAlignment="Center" Height="Auto" VerticalAlignment="Top">
							<!--Third grid column and row definitions. If you compare these to the 2nd grid's column definitions, you can see that these columns (and rows) have their dimensions specifically defined.-->
							<Grid.RowDefinitions>
								<RowDefinition Height="0" />  <!-- COLLAPSED ROW TO AVOID EDITING ALL ROWS -->
								<RowDefinition Height="38" />
								<RowDefinition Height="38" />
								<RowDefinition Height="38" />
								<RowDefinition Height="38" />
								<RowDefinition Height="38" />
								<RowDefinition Height="38" />
								<RowDefinition Height="38" />
								<RowDefinition Height="38" />
								<RowDefinition Height="38" />
								<RowDefinition Height="38" />
								<RowDefinition Height="38" />
								<RowDefinition Height="38" />
								<RowDefinition Height="38" />
								<RowDefinition Height="38" />
								<RowDefinition Height="38" />
								<RowDefinition Height="38" />
								<RowDefinition Height="38" />
								<RowDefinition Height="38" />
								<RowDefinition Height="38" />
								<RowDefinition Height="38" />
							</Grid.RowDefinitions>
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="20" />
								<ColumnDefinition Width="145" />
								<ColumnDefinition Width="30" />
								<ColumnDefinition Width="70" />
								<ColumnDefinition Width="70" />
								<ColumnDefinition Width="70" />
								<ColumnDefinition Width="70" />
							</Grid.ColumnDefinitions>

							<Button Grid.Column="0" Focusable="False" DataContext="'key':'Space'" Name="Toggle_1" />

							<!--Here is where the race data is inserted into the grid. This example is all done with labels exept for the racer's name which is a textbox control. An easy way to visualize what's going on here is to remember that the rows represent the individual drivers and the columns represent the data for those drivers. An exception is the top row which is simply displaying headers for the data. To see how things change here, try changing the "Foreground" property of the first line to "Black", then save the file and load it as the Race UI in RC. Did you see the change?-->

							<!--Labels for Column Headers-->
							<Label Padding="0,-3,0,0" Grid.Column="0" Grid.Row="0" Background="DarkGray" BorderBrush="DarkGray" BorderThickness="1" FontSize="10" HorizontalContentAlignment="Center" VerticalContentAlignment="Top" Content="Pos" Foreground="White" FontStyle="Normal" FontWeight="Bold" />
							<Label Padding="0,-3,0,0" Grid.Column="1" Grid.Row="0" Background="DarkGray" BorderBrush="DarkGray" BorderThickness="1" FontSize="10" HorizontalContentAlignment="Center" VerticalContentAlignment="Top" Content="Driver" Foreground="White" FontStyle="Normal" FontWeight="Bold" />
							<Label Padding="0,-3,0,0" Grid.Column="2" Grid.Row="0" Background="DarkGray" BorderBrush="DarkGray" BorderThickness="1" FontSize="8" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Content="Lap" Foreground="White" FontStyle="Normal" FontWeight="Bold" />
							<Label Padding="0,-3,0,0" Grid.Column="3" Grid.Row="0" Background="DarkGray" BorderBrush="DarkGray" BorderThickness="1" FontSize="10" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="White" Content="Avg Time" FontStyle="Normal" FontWeight="Bold" />
							<Label Padding="0,-3,0,0" Grid.Column="4" Grid.Row="0" Background="DarkGray" BorderBrush="DarkGray" BorderThickness="1" FontSize="9" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="White" Content="Best Time" FontWeight="Bold" FontStyle="Normal" />
							<Label Padding="0,-3,0,0" Grid.Column="5" Grid.Row="0" Background="DarkGray" BorderBrush="DarkGray" BorderThickness="1" FontSize="10" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="White" Content="Gap 1st" FontWeight="Bold" FontStyle="Normal" />
							<Label Padding="0,-3,0,0" Grid.Column="6" Grid.Row="0" Background="DarkGray" BorderBrush="DarkGray" BorderThickness="1" FontSize="10" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Foreground="White" Content="Gap" FontStyle="Normal" FontWeight="Bold" />
							<!--End of labels for Column Headers-->

							<!--Row for first position racer-->
							<!-- DRIVER POSITION -->
							<Label Grid.Row ="1" Grid.Column="0" Foreground="Transparent" Background="White" BorderBrush="{StaticResource StatusGridBorderBrush}" BorderThickness="2,2,.5,.5" FontSize="38" HorizontalContentAlignment="Center" 
                    VerticalContentAlignment="Center" Name="Pos1_2" Content=" 1" />
							<Viewbox Margin="2" VerticalAlignment="Center" Stretch="Fill" Grid.Row ="1" Grid.Column="0" MaxWidth="18" MaxHeight="18">
								<Label Background="{StaticResource StatusGridBackground}" BorderBrush="Transparent" BorderThickness="2,2,.5,.5" FontSize="18" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Name="Pos1_1" Content=" 1" />
							</Viewbox>
							<!-- END OF DRIVER POSITION -->
							<!-- DRIVER NAME -->
							<Label Grid.Row="1" Grid.Column="1" Background="White" BorderBrush="Black" BorderThickness=".5,2,.5,.5" Foreground="Transparent" Name="Nickname_RaceLeader1_2"/>
							<Viewbox Margin="2" VerticalAlignment="Center" Stretch="Fill" MaxWidth="348"  MaxHeight="98" MinHeight="20" Grid.Row="1" Grid.Column="1">
								<Label Background="{StaticResource StatusGridBackground}" BorderBrush="Transparent" BorderThickness=".5,2,.5,.5" FontFamily="Tahoma" FontSize="20" FontWeight="Bold" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Name="Nickname_RaceLeader1_1" />
							</Viewbox>
							<!-- Display the fuel image surrounded by a viewbox to make sure it fits in the space we have 
                    <Label Grid.Row="1" Grid.Column="2" Background="Black" BorderBrush="Black" BorderThickness="1.5" Foreground="Transparent" Name="Nickname_Position1_3"/>
                    <Viewbox MaxWidth="97"  MaxHeight="98" MinHeight="38" Grid.Row="1" Grid.Column="2">
                        <Image Stretch="Fill" Name="FuelImage_Position1_1" />
                    </Viewbox -->
							<!-- LAP COUNT -->
							<Label Grid.Row ="1" Grid.Column="2" Foreground="Transparent" Background="White" BorderBrush="Black" BorderThickness=".5,2,.5,.5" FontSize="38" HorizontalContentAlignment="Center" 
                    VerticalContentAlignment="Center" Name="Lap_RaceLeader1_2" />
							<Viewbox Margin="2" VerticalAlignment="Center" Stretch="Fill" Grid.Row ="1" Grid.Column="2" MaxWidth="98" MaxHeight="98">
								<Label Background="{StaticResource StatusGridBackground}" BorderBrush="Transparent" BorderThickness=".5,2,.5,.5" FontSize="38" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Name="Lap_RaceLeader1_1" DataContext="'decimals':0"/>
							</Viewbox>
							<!-- ADD LAPS -->
							<Button Grid.Row ="1" Grid.Column="2" Name="AddLapSections_Position1_1" Opacity="0" IsTabStop="False" Focusable="False" />
							<StackPanel Grid.Row ="1" Grid.Column="2" Margin="4, 4, 4, 4" Orientation="Horizontal" HorizontalAlignment="Right" VerticalAlignment="Bottom">
								<Image Width="15" Height="15" Name="DriftLap_Position1_1" Stretch="Fill" />
							</StackPanel>
							<!-- End of LAP COUNT Row  -->
							<!-- AVERAGE LAP TIME -->
							<Label Grid.Row="1" Grid.Column="3" Foreground="Transparent" Background="White" BorderBrush="Black" BorderThickness=".5,2,.5,.5" FontSize="38" HorizontalContentAlignment="Center" 
